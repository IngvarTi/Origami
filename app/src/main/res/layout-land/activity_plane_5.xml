<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffff"
    tools:context="com.mittereflores.origami.Plane_5_Activity"
    android:id="@+id/actPlane_5">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/scrollView"
        android:layout_weight="1">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="@dimen/button_image_size_land_width"
                android:layout_height="@dimen/button_image_size_land_height"
                android:id="@+id/vectorImage"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPlane_5_1"
                android:id="@+id/textMovingPlane51"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_land_width"
                android:layout_height="@dimen/button_image_size_land_height"
                android:id="@+id/vectorImage1"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPlane_5_2"
                android:id="@+id/textMovingPlane52"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_land_width"
                android:layout_height="@dimen/button_image_size_land_height"
                android:id="@+id/vectorImage2"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPlane_5_3"
                android:id="@+id/textMovingPlane53"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_land_width"
                android:layout_height="@dimen/button_image_size_land_height"
                android:id="@+id/vectorImage3"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPlane_5_4"
                android:id="@+id/textMovingPlane54"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_land_width"
                android:layout_height="@dimen/button_image_size_land_height"
                android:id="@+id/vectorImage4"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPlane_5_5"
                android:id="@+id/textMovingPlane55"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_land_width"
                android:layout_height="@dimen/button_image_size_land_height"
                android:id="@+id/vectorImage5"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPlane_5_6"
                android:id="@+id/textMovingPlane56"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_land_width"
                android:layout_height="@dimen/button_image_size_land_height"
                android:id="@+id/vectorImage6"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPlane_5_7"
                android:id="@+id/textMovingPlane57"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_land_width"
                android:layout_height="@dimen/button_image_size_land_height"
                android:id="@+id/vectorImage7"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/finished"
                android:id="@+id/textMovingPlane58"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_margin" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>
