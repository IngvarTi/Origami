<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffff"
    tools:context="com.mittereflores.origami.Moving_6_Activity"
    android:id="@+id/actMoving_6">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/scrollView"
        android:layout_weight="1">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="@dimen/button_image_size_600_width"
                android:layout_height="@dimen/button_image_size_600_height"
                android:id="@+id/vectorImage"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPakupaku_1"
                android:id="@+id/textMovingBird1"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_600_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_600_width"
                android:layout_height="@dimen/button_image_size_600_height"
                android:id="@+id/vectorImage1"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPakupaku_2"
                android:id="@+id/textMovingBird2"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_600_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_600_width"
                android:layout_height="@dimen/button_image_size_600_height"
                android:id="@+id/vectorImage2"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPakupaku_3"
                android:id="@+id/textMovingBird3"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_600_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_600_width"
                android:layout_height="@dimen/button_image_size_600_height"
                android:id="@+id/vectorImage3"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPakupaku_4"
                android:id="@+id/textMovingBird4"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_600_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_600_width"
                android:layout_height="@dimen/button_image_size_600_height"
                android:id="@+id/vectorImage4"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPakupaku_5"
                android:id="@+id/textMovingBird5"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_600_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_600_width"
                android:layout_height="@dimen/button_image_size_600_height"
                android:id="@+id/vectorImage5"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textPakupaku_6"
                android:id="@+id/textMovingBird6"
                android:textSize="@dimen/button_name_size_height"
                android:layout_gravity="center_horizontal"
                android:textColor="#000000"
                android:layout_marginBottom="@dimen/button_image_vertical_600_margin" />

            <ImageView
                android:layout_width="@dimen/button_image_size_600_width"
                android:layout_height="@dimen/button_image_size_600_height"
                android:id="@+id/vectorImage6"
                android:layout_gravity="center_horizontal" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>
